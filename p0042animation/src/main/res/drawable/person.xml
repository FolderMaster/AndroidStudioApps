<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" android:height="100dp"
    android:width="50dp">
    <item android:left="0dp" android:top="0dp" android:width="50dp" android:height="80dp">
        <shape android:shape="rectangle">
            <solid android:color="#00FFFFFF"/>
        </shape>
    </item>
    <item android:left="20dp" android:top="10dp" android:width="10dp" android:height="50dp">
        <shape android:shape="rectangle">
            <corners android:radius="15dp"/>
            <solid android:color="#FF000000"/>
        </shape>
    </item>
    <item android:left="15dp" android:top="0dp" android:width="20dp" android:height="20dp">
        <shape android:shape="oval">
            <solid android:color="#FF000000"/>
        </shape>
    </item>
    <item android:left="10dp" android:top="20dp" android:width="10dp" android:height="30dp">
        <rotate android:fromDegrees="45" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="rectangle">
                <corners android:radius="10dp"/>
                <solid android:color="#FF000000"/>
            </shape>
        </rotate>
    </item>
    <item android:left="30dp" android:top="20dp" android:width="10dp" android:height="30dp">
        <rotate android:fromDegrees="-45" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="rectangle">
                <corners android:radius="10dp"/>
                <solid android:color="#FF000000"/>
            </shape>
        </rotate>
    </item>
    <item android:left="13dp" android:top="50dp" android:width="10dp" android:height="40dp">
        <rotate android:fromDegrees="25" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="rectangle">
                <corners android:radius="10dp"/>
                <solid android:color="#FF000000"/>
            </shape>
        </rotate>
    </item>
    <item android:left="27dp" android:top="50dp" android:width="10dp" android:height="40dp">
        <rotate android:fromDegrees="-25" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="rectangle">
                <corners android:radius="10dp"/>
                <solid android:color="#FF000000"/>
            </shape>
        </rotate>
    </item>
</layer-list>